{
  "kind":"Deployment",
  "apiVersion":"apps/v1",
  "metadata":{
     "name":"05capstone",
     "labels":{
        "app":"05capstone"
     }
  },
  "spec":{
     "replicas":2,
     "selector":{
        "matchLabels":{
           "app":"05capstone"
        }
     },
     "template":{
        "metadata":{
           "labels":{
              "app":"05capstone"
           }
        },
        "spec":{
           "containers":[
              {
                 "name":"05_project_capstone",
                 "image":"twdockeruser/05_project_capstone",
                 "ports":[
                    {
                       "name":"http-server",
                       "containerPort":80
                    }
                 ]
              }
           ]
        }
     }
  }
}